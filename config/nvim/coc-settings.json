{
  "suggest.echodocSupport": true,
  // diagnostic
  "diagnostic.infoSign":            "ღ",
  "diagnostic.hintSign":            "ஐ",
  "diagnostic.warningSign":         "⚠",
  "diagnostic.errorSign":           "✖",
  "diagnostic.enable":              true,
  // Java home
  "xml.java.home":"/usr/lib/jvm/java-17-openjdk-amd64",
  "languageserver": {
    "d": {
      "command": "~/.dub/packages/.bin/dls-latest/dls", // this path should be printed with the previous commands
      "args": ["--stdio"],
      "filetypes": ["d"],
      "trace.server": "off",
      "rootPatterns": ["dub.json", "dub.sdl"],
      "initializationOptions": {
      },
      "settings": {
      }
    }
  }
}
